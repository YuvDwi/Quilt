{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "uvicorn simple_health_check:app --host 0.0.0.0 --port $PORT --timeout-keep-alive 300",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 120,
    "healthcheckInterval": 10,
    "restartPolicyType": "ON_FAILURE"
  }
}
